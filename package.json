{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "validate-books": "node ./operations/validation/validateContent.js",
    "validate-authors": "yarn ajv validate -s ./schemas/author.json -d \"./content/authors/*.yaml\"",
    "validate-series": "yarn ajv validate -s ./schemas/series.json -d \"./content/series/*.yaml\"",
    "validate": "yarn run validate-books && yarn run validate-authors && yarn run validate-series"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "ajv": "^6.12.5",
    "bootstrap": "^4.5.2",
    "colors": "^1.4.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "gatsby": "^2.24.54",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-google-analytics": "^2.3.14",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sass": "^2.3.13",
    "gatsby-source-filesystem": "^2.3.29",
    "gatsby-transformer-yaml": "^2.4.11",
    "gatsby-transformer-yaml-full": "^0.3.4",
    "gatsby-yaml-full-markdown": "^0.3.4",
    "glob": "^7.1.6",
    "ical-generator": "^1.15.0",
    "log-symbols": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-share": "^4.3.0",
    "reactstrap": "^8.6.0",
    "sass": "^1.26.10",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.14",
    "@types/react-helmet": "^6.1.0",
    "@types/reactstrap": "^8.5.1",
    "ajv-cli": "^3.3.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
